<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:aop="http://www.springframework.org/schema/aop"
	   xmlns:context="http://www.springframework.org/schema/context"
	   xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
        http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-3.1.xsd
        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.1.xsd">

	<!-- Example resource repositories -->
	<bean id="katharsis_movieRepository" class="io.katharsis.example.spring.jersey.impl.MovieRepositoryImpl" />
	<bean id="katharsis_actorRepository" class="io.katharsis.example.spring.jersey.impl.ActorRepositoryImpl" />

	<!-- Example relationship resource repositories -->
	<bean id="katharsis_movieToActorRepositoryImpl" class="io.katharsis.example.spring.jersey.impl.MovieToActorRelationshipRepositoryImpl">
		<property name="movieRepository" ref="katharsis_movieRepository" />
	</bean>


</beans>
